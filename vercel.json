{
  "version": 2,
  "builds": [
    {
      "src": "src/app/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "src/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "src/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/app/app.js"
    }
  ]
}